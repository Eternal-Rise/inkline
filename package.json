    "scripts": {
        "dev": "npm run storybook",
        "build": "rimraf lib lib-cjs dist && run-p build:*",
        "build:unbuild": "unbuild",
        "build:vite": "vite build",
        "postbuild": "ts-node ./scripts/postbuild.ts",
        "lint": "run-p lint:script lint:style lint:markdown",
        "lint:script": "eslint --ext .vue,.js,.ts src",
        "lint:prettier": "prettier --write --loglevel warn \"**/*.{js,json,css,scss,vue,html}\"",
        "lint:style": "stylelint \"src/**/*.{scss,vue}\"",
        "lint:markdown": "markdownlint '**/*.md'",
        "prerelease": "npm run lint && npm run test && npm run build",
        "release": "np",
        "postrelease": "ts-node scripts/postrelease.ts",
        "postversion": "ts-node scripts/postversion.ts",
        "test": "vitest run --coverage",
        "test:dev": "vitest",
        "test:dev:ui": "vitest --ui",
        "coveralls": "cat coverage/lcov.info | coveralls",
        "generate:config": "inkline generate scss --outputDir ./src/css/variables",
        "generate:exports": "ts-node ./scripts/generate/exports.ts",
        "generate:utilities": "ts-node ./scripts/generate/utilities.ts",
        "generate:manifest": "ts-node ./scripts/manifest.ts",
        "generate:manifest:sass": "ts-node ./scripts/manifest-sass.ts",
        "storybook": "start-storybook -p 6006",
        "storybook:build": "build-storybook",
        "stylelint-check": "stylelint-config-prettier-check",
        "postinstall": "rimraf ./node_modules/@types/react & rimraf ./node_modules/react/src/**/*.d.ts"
    },
